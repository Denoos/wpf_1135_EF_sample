<Window x:Class="wpf_1135_EF_sample.YellowPressAdd"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:wpf_1135_EF_sample"
        mc:Ignorable="d"
        Title="YellowPressAdd" Height="450" Width="800">
    <StackPanel Orientation="Vertical">
        <StackPanel Orientation="Horizontal">
            <Label Content="Кликбейт заголовок: " VerticalContentAlignment="Top"/>
            <TextBox x:Name="Zag" Width="650" Text="{Binding YP.TitleArticle}"/>
        </StackPanel>

        <StackPanel Orientation="Horizontal">
            <Label Content="Треш-статья: " VerticalContentAlignment="Top"/>
            <TextBox x:Name="Des" Width="650" Text=""/>
        </StackPanel>

        <StackPanel Orientation="Horizontal">
            <Label Content="Айди жертвы: " VerticalContentAlignment="Top"/>
            <TextBox x:Name="Ids" Width="650" Text=""/>
        </StackPanel>

        <Button Content="Назад" Margin="20, 20, 680, 10" Click="SaveAnBack"/>
    </StackPanel>
</Window>
